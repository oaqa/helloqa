		</dependency>
		<dependency>
			<groupId>eij</groupId>
			<artifactId>stanford-parser</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>eij</groupId>
			<artifactId>opennlp-tool</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>eij</groupId>
			<artifactId>wikixmlj</artifactId>
			<version>r43</version>
		</dependency>
		<dependency>
			<groupId>org.tartarus.snowball</groupId>
			<artifactId>stemmer</artifactId>
			<version>20120530</version>
		</dependency>
		<dependency>
			<groupId>eij</groupId>
			<artifactId>simmetric</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>eij</groupId>
			<artifactId>racr</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>eij</groupId>
			<artifactId>racr-libx</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>eij</groupId>
			<artifactId>uimaj-as-activemq</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>eij</groupId>
			<artifactId>uima-as-core</artifactId>
			<version>2.4</version>
		</dependency>
		<dependency>
			<groupId>eij</groupId>
			<artifactId>annotation-uima-jms</artifactId>
			<version>1.0</version>
		</dependency>						
		<dependency>
			<groupId>edu.cmu.minorthird</groupId>
			<artifactId>minorthird</artifactId>
			<version>20080611</version>
		</dependency>
		<dependency>
			<groupId>joda-time</groupId>
			<artifactId>joda-time</artifactId>
			<version>2.2</version>
		</dependency>
		<dependency>
			<groupId>com.hp.hpl.jena</groupId>
			<artifactId>jena</artifactId>
			<version>2.6.4</version>
		</dependency>
		<dependency>
			<groupId>edu.cmu.lti.javelin</groupId>
			<artifactId>javelin</artifactId>
			<version>1.0.0</version>
		</dependency>
		<dependency>
			<groupId>org.lemurproject</groupId>
			<artifactId>indri</artifactId>
			<version>5.2</version>
		</dependency>
		<dependency>
			<groupId>eij</groupId>
			<artifactId>ephyra</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>edu.cmu.lti.oaqa.framework.impl</groupId>
			<artifactId>jdbc-providers</artifactId>
			<version>2.3.2</version>
		</dependency>
		<dependency>
			<groupId>org.xerial</groupId>
			<artifactId>sqlite-jdbc</artifactId>
			<version>3.7.2</version>
		</dependency>
		<dependency>
			<groupId>edu.cmu.lti.oaqa.bio.core</groupId>
			<artifactId>baseqa</artifactId>
			<version>1.9.1</version>
		</dependency>
		<dependency>
			<groupId>edu.cmu.lti.oaqa.core.provider</groupId>
			<artifactId>solr-provider</artifactId>
			<version>1.0.7</version>
		</dependency>
		<dependency>
			<groupId>com.google.guava</groupId>
			<artifactId>guava</artifactId>
			<version>13.0</version>
		</dependency>
		<dependency>
			<groupId>com.hp.hpl.jena</groupId>
			<artifactId>arq</artifactId>
			<version>2.8.8</version>
		</dependency>
		<dependency>
			<groupId>net.sf.opencsv</groupId>
			<artifactId>opencsv</artifactId>
			<version>2.3</version>
		</dependency>
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>slf4j-api</artifactId>
			<version>1.7.5</version>
		</dependency>
		<dependency>
			<groupId>org.slf4j</groupId>
			<artifactId>slf4j-log4j12</artifactId>
			<version>1.7.5</version>
		</dependency>
		<dependency>
			<groupId>edu.cmu.lti.oaqa.ecd</groupId>
			<artifactId>uima-ecd</artifactId>
			<version>2.3.1</version>
		</dependency>
		<dependency>
			<groupId>net.sf.jwordnet</groupId>
			<artifactId>jwnl</artifactId>
			<version>1.4_rc3</version>
		</dependency>
		<dependency>
			<groupId>com.aliasi</groupId>
			<artifactId>lingpipe</artifactId>
			<version>4.1.0</version>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<configuration>
					<source>1.6</source>
					<target>1.6</target>
				</configuration>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-release-plugin</artifactId>
				<version>2.2.1</version>
				<executions>
					<execution>
						<id>default</id>
						<goals>
							<goal>perform</goal>
						</goals>
						<configuration>
							<pomFileName>hw1-amaiberg/pom.xml</pomFileName>
						</configuration>
					</execution>
				</executions>
			</plugin>
			<plugin>
				<artifactId>maven-assembly-plugin</artifactId>
				<configuration>
					<archive>
						<manifest>
							<mainClass>edu.cmu.lti.oaqa.ecd.driver.ECDDriver</mainClass>
						</manifest>
					</archive>
					<descriptorRefs>
						<descriptorRef>jar-with-dependencies</descriptorRef>
					</descriptorRefs>
				</configuration>
			</plugin>
		</plugins>
	</build>
</project>
